<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#0099ff">

        <ImageView
            android:id="@+id/yonghuImage"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="18dp"
            android:layout_marginStart="18dp"
            android:background="#00000000" />

        <TextView
            android:id="@+id/yonghuText"
            android:layout_width="wrap_content"
            android:layout_height="20dp"

            android:layout_alignTop="@+id/yonghuImage"
            android:layout_marginLeft="17dp"
            android:layout_marginStart="17dp"
            android:layout_toEndOf="@+id/yonghuImage"
            android:layout_toRightOf="@+id/yonghuImage"
            android:background="#00000000"
            android:text="用户"
            android:textColor="#FFFFFF"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/yonghuPho"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/yonghuImage"
            android:layout_alignLeft="@+id/yonghuText"
            android:layout_alignStart="@+id/yonghuText"
            android:text="手机号"
            android:textColor="#ffffff"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/xxxx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/yonghuPho"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="43dp"
            android:layout_marginRight="43dp"
            android:text="查看个人信息"
            android:textColor="#ffffff" />

        <ImageView
            android:id="@+id/jiantou"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignBottom="@+id/xxxx"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="27dp"
            android:layout_marginRight="27dp"
            app:srcCompat="@drawable/jiantou2" />
    </RelativeLayout>
    <com.example.danganguan.BorderRelativeLayout
        android:id="@+id/myyy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/relativeLayout"
        android:layout_marginTop="13dp"
        android:background="@drawable/shape_corners"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/jiantou0"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp"
            app:srcCompat="@drawable/jiantou" />

        <ImageView
            android:id="@+id/myyyImag"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/myyy" />

        <TextView
            android:id="@+id/myyyText"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/myyyImag"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_toEndOf="@+id/myyyImag"
            android:layout_toLeftOf="@+id/jiantou0"
            android:layout_toRightOf="@+id/myyyImag"
            android:layout_toStartOf="@+id/jiantou0"
            android:layout_weight="4"
            android:text="我的预约"
            android:textSize="20dp" />
    </com.example.danganguan.BorderRelativeLayout>

    <com.example.danganguan.BorderRelativeLayout
        android:id="@+id/wuliu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/myyy"
        android:background="@drawable/shape_corners"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/jiantou1"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp"
            app:srcCompat="@drawable/jiantou" />

        <ImageView
            android:id="@+id/wuliuImag"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/wl" />

        <TextView
            android:id="@+id/wuliuText"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/wuliuImag"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_toEndOf="@+id/wuliuImag"
            android:layout_toLeftOf="@+id/jiantou1"
            android:layout_toRightOf="@+id/wuliuImag"
            android:layout_toStartOf="@+id/jiantou1"
            android:layout_weight="4"
            android:text="查看物流"
            android:textSize="20dp" />
    </com.example.danganguan.BorderRelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/shape_corners"
        android:layout_below="@+id/wuliu"
        android:layout_marginTop="13dp"
        android:orientation="vertical"
        android:id="@+id/linearLayout">

        <com.example.danganguan.BorderRelativeLayout
            android:id="@+id/gaitou"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/wuliu"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/jiantou2"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="17dp"
                android:layout_marginRight="17dp"
                app:srcCompat="@drawable/jiantou" />

            <ImageView
                android:id="@+id/gaitouImage"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/gaitou" />

            <TextView
                android:id="@+id/gaitouText"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/gaitouImage"
                android:layout_toLeftOf="@+id/jiantou2"
                android:layout_toRightOf="@+id/gaitouImage"
                android:layout_toStartOf="@+id/jiantou2"
                android:layout_weight="4"
                android:text="修改头像"
                android:textSize="20dp" />
        </com.example.danganguan.BorderRelativeLayout>

        <com.example.danganguan.BorderRelativeLayout
            android:id="@+id/gaini"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/gaitou"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/jiantou3"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="17dp"
                android:layout_marginRight="17dp"
                app:srcCompat="@drawable/jiantou" />

            <ImageView
                android:id="@+id/gainiImage"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/gaini" />

            <TextView
                android:id="@+id/gainiText"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/gainiImage"
                android:layout_toLeftOf="@+id/jiantou3"
                android:layout_toRightOf="@+id/gainiImage"
                android:layout_toStartOf="@+id/jiantou3"
                android:layout_weight="4"
                android:text="修改昵称"
                android:textSize="20dp" />
        </com.example.danganguan.BorderRelativeLayout>

        <com.example.danganguan.BorderRelativeLayout
            android:id="@+id/gaimi"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/gaini"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/jiantou4"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="17dp"
                android:layout_marginRight="17dp"
                app:srcCompat="@drawable/jiantou" />

            <ImageView
                android:id="@+id/gaimiImage"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/gaimi" />

            <TextView
                android:id="@+id/gaimiText"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_alignTop="@+id/gaimiImage"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/gaimiImage"
                android:layout_toRightOf="@+id/gaimiImage"
                android:layout_toLeftOf="@+id/jiantou4"
                android:layout_toStartOf="@+id/jiantou4"
                android:layout_weight="4"
                android:text="修改密码"
                android:textSize="20dp" />
        </com.example.danganguan.BorderRelativeLayout>
    </LinearLayout>

    <com.example.danganguan.BorderRelativeLayout
        android:id="@+id/tc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/linearLayout"
        android:background="@drawable/shape_corners"
        android:layout_marginTop="16dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/jiantou5"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp"
            app:srcCompat="@drawable/jiantou" />

        <ImageView
            android:id="@+id/tcImage"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/tc" />

        <TextView
            android:id="@+id/tcText"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:layout_toEndOf="@+id/tcImage"
            android:layout_toLeftOf="@+id/jiantou5"
            android:layout_toRightOf="@+id/tcImage"
            android:layout_toStartOf="@+id/jiantou5"
            android:layout_weight="4"
            android:text="退出登陆"
            android:textSize="20dp" />
    </com.example.danganguan.BorderRelativeLayout>


</RelativeLayout>

