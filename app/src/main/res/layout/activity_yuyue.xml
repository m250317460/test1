<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:scrollbars="vertical"
        android:layout_width="match_parent">
        <View
            android:id="@+id/paddingView1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#000"
            />
        <com.example.danganguan.TitleLayout2
            android:id="@+id/titlelayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/paddingView1"
            android:background="#00ccff">

        </com.example.danganguan.TitleLayout2>

        <ExpandableListView
            android:id="@+id/exlist_lol"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/titlelayout1"/>

        <TextView
            android:id="@+id/xztime"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_below="@+id/exlist_lol"
            android:gravity="center_vertical|center"
            android:text="选择时间" />
        <Button
            android:id="@+id/yyqueren"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/xztime"
            android:layout_marginTop="20sp"
            android:textColor="#FFFFFF"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#00ccff"
            android:text="确认预约" />

    </RelativeLayout>
